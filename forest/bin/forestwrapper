#!/bin/bash 

export LLVM_HOME=/forest/tools/llvm-2.9
export FOREST_HOME=/forest
export CPACHECKER_HOME=/home/mint/CPAchecker-1.3.4-unix
export PATH="/forest/bin/:$PATH"
export LD_LIBRARY_PATH=/usr/local/lib/

/forest/bin/forest -svcomp -file /tmp/test.c | awk '{print $5}' | sed -e 's/^R://g' -e 's/^.....//g' -e 's/....$//g'

